{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Listen for Xdebug",
      "type": "php",
      "request": "launch",
      "port": 9011,
      "pathMappings": {
        "/var/www/html": "${workspaceFolder}/src"
      },
      "xdebugSettings": {
        "max_children": 128,
        "max_data": 512,
        "max_depth": 5
      }
    }
  ]
}
